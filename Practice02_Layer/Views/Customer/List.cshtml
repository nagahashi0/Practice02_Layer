@model Practice02_Layer.Models.Customer.ListViewModel



<h2>List</h2>


@using (Html.BeginForm("SearchList", "Customer", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <h4>@Model.PageTitle</h4>
    <hr />
   
    <div class="form-group">
        <div class="col-md-10">
           @Html.EditorFor(mdl => mdl.FilterItems.CustomerName, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="送信" />
        </div>
    </div>
}
